{
  "name": "roks-rjs-palawanuicomponents",
  "version": "0.1.9",
  "description": "A React component library",
  "repository": {
    "type": "git",
    "url": "https://github.com/rokku-x/roks-rjs-palawanuicomponents.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.es.d.ts",
  "bin": {
    "render-logo": "scripts/render-logo.js",
    "roks-rjs-palawanuicomponents": "scripts/render-logo.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.es.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    },
    "./style.css": "./dist/roks-rjs-palawanuicomponents.css",
    "./PalawanPayLogo": {
      "types": "./dist/PalawanPayLogo/index.d.ts",
      "import": "./dist/PalawanPayLogo/index.esm.js",
      "require": "./dist/PalawanPayLogo/index.js"
    },
    "./PGCLogo": {
      "types": "./dist/PGCLogo/index.d.ts",
      "import": "./dist/PGCLogo/index.esm.js",
      "require": "./dist/PGCLogo/index.js"
    },
    "./PEPPLogo": {
      "types": "./dist/PEPPLogo/index.d.ts",
      "import": "./dist/PEPPLogo/index.esm.js",
      "require": "./dist/PEPPLogo/index.js"
    },
    "./PPSLogo": {
      "types": "./dist/PPSLogo/index.d.ts",
      "import": "./dist/PPSLogo/index.esm.js",
      "require": "./dist/PPSLogo/index.js"
    },
    "./PPSPEPPLogo": {
      "types": "./dist/PPSPEPPLogo/index.d.ts",
      "import": "./dist/PPSPEPPLogo/index.esm.js",
      "require": "./dist/PPSPEPPLogo/index.js"
    },
    "./PPSPEPPPPayLogo": {
      "types": "./dist/PPSPEPPPPayLogo/index.d.ts",
      "import": "./dist/PPSPEPPPPayLogo/index.esm.js",
      "require": "./dist/PPSPEPPPPayLogo/index.js"
    },
    "./PPayText": {
      "types": "./dist/PPayText/index.d.ts",
      "import": "./dist/PPayText/index.esm.js",
      "require": "./dist/PPayText/index.js"
    },
    "./PalawanLoading": {
      "types": "./dist/PalawanLoading/index.d.ts",
      "import": "./dist/PalawanLoading/index.esm.js",
      "require": "./dist/PalawanLoading/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist",
    "src/components/**/*.svg",
    "src/components/**/*.jpg",
    "src/components/**/*.png"
  ],
  "scripts": {
    "build": "vite build",
    "render:logo": "node ./scripts/render-logo.js",
    "dev": "vite",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.17",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.22",
    "jsdom": "^27.2.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.0.0",
    "vite": "^7.1.9",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.15",
    "sharp": "^0.32.0",
    "esbuild-register": "^3.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "keywords": [
    "react",
    "component",
    "library"
  ],
  "author": "rokku",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}